{
	"name": "example",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"build:ww": "tsup ./workers/web-worker.ts --format iife --dts --sourcemap --out-dir ./public",
		"dev:ww": "tsup ./workers/web-worker.ts --format iife --dts --sourcemap --out-dir ./public --watch",
		"build:sw": "tsup ./workers/service-worker.ts --format iife --dts --sourcemap --out-dir ./public",
		"dev:sw": "tsup ./workers/service-worker.ts --format iife --dts --sourcemap --out-dir ./public --watch"
	},
	"dependencies": {
		"auth-worker": "file:../dist",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^15.0.2",
		"@types/react": "^18.2.5",
		"@types/react-dom": "^18.2.3",
		"@vitejs/plugin-react-swc": "^3.3.0",
		"rollup": "^3.21.4",
		"rollup-plugin-typescript": "^1.0.1",
		"typescript": "^5.0.4",
		"vite": "^4.3.4"
	}
}
